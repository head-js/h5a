    "dist": "uglifyjs --screw-ie8 --stats --mangle --verbose --compress --output ./dist/integration.min.js ./dist/integration.js && gsed -i '1s/^/\\/* analytics.js-integration-lattebank-analytics 0.1.7 *\\/\\n/' ./dist/integration.min.js"
  },
  "dependencies": {
    "@ndhoule/defaults": "2.0.1",
