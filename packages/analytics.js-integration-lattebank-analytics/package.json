    "obj-case": "0.2.0",
    "vanilla.js": "0.0.10"
  },
  "peerDependencies": {
    "@lattebank/analytics.js-core": "3.0.0"
  },
  "devDependencies": {
    "rollup": "0.31.2",
    "rollup-plugin-commonjs": "3.0.0",
    "rollup-plugin-node-resolve": "1.7.0",
    "uglify-js": "2.7.0"
  }
