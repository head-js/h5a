{
  "name": "@lattebank/analytics.js-integration-lattebank-analytics",
  "version": "0.2.0",
  "description": "The Latte Bank Stats Analytics.js Integration.",
  "author": "徐帅哲<xushuaizhe@lattebank.com>",
